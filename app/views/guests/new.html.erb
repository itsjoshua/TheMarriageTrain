
<div id="wish_form">
	<%= form_for(@guest) do |f| %>
		<%= render "shared/error_messages" %>
		<%= f.label :name %>
		<%= f.text_field :name %>
		<br>
		<%= f.label :email %>
		<%= f.text_field :email %>
		<br>
		<%= f.label :wish %>
		<%= f.text_field :wish %>
		<br>
		<%= f.submit "Submit wish" %>
		
	<% end %>
</div>
